<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="syntax.xslt"?>
<XMLConfigSettings>
  <FILEINFO author="Evi" type="LangSpec"/>
  <SYNTAX id="clipper" name="Clipper" inherit="def_source" version="1.2" inherit_url="defsource_spec.xml">
    <SPECIFICATION>
      <FilePattern mask="*.prg;*.ch">
        <Pair file1="*.prg" file2="*.ch"/>
      </FilePattern>
      <SyntaxBlocks>
        <Block open="#include "/>
      </SyntaxBlocks>
      <OpenClose>()[]{}''""||</OpenClose>
      <Operators>#@,;.:?--+/&amp;%*=&lt;&gt;~!^()|\[]{}</Operators>
      <Delimiters>&quot;</Delimiters>
      <Words>0-9A-Za-z_</Words>
      <Bars navigation="true"/>
    </SPECIFICATION>
    <SCOPES>
      <Scope open="FUNCTION" close="RETURN" strict="false"/>
      <Scope open="PROCEDURE" close="RETURN" strict="false"/>
      <Scope open="TEXT" close="ENDTEXT"/>
      <Scope open="IF" middle="ELSE" strict="false">
        <Close text="END" trail="\s+?(:?/|$)"/>
        <Close text="ENDIF"/>
        <Middle text="ELSEIF"/>
      </Scope>
      <Scope open="BEGIN SEQUENCE" middle="RECOVER" close="END SEQUENCE"/>
      <Scope open="WHILE" strict="false">
        <Close text="END" trail="\s+?(:?/|$)"/>
        <Close text="ENDDO"/>
      </Scope>
      <Scope open="DO WHILE" close="ENDDO"/>
      <Scope open="MENU" close="ENDMENU" strict="false"/>
      <Scope open="FOR" close="NEXT" strict="false"/>
      <Scope open="FOR EACH" close="NEXT"/>
      <Scope open="WITH OBJECT" close="ENDWITH"/>
      <Scope open="DO CASE" middle="CASE" close="ENDCASE">
        <Middle text="OTHERWISE"/>
        <Close text="END CASE"/>
      </Scope>
      <Scope open="SWITCH" middle="CASE" close="ENDSWITCH">
        <Middle text="DEFAULTSWITCH"/>
        <Middle text="DEFAULT"/>
      </Scope>
      <Scope open="TRY" middle="CATCH" close="ENDTRY">
        <Middle text="FINALLY"/>
      </Scope>
      <Scope open="IFDEF" middle="ELSE" close="ENDIF">
        <Containers>
          <Open id="normal"/>
          <Open id="preprocessor"/>
        </Containers>
      </Scope>
      <Scope open="IFNDEF" middle="ELSE" close="ENDIF">
        <Containers>
          <Open id="normal"/>
          <Open id="preprocessor"/>
        </Containers>
      </Scope>
      <Scope open="Region" close="EndRegion"/>
    </SCOPES>
    <LABELS>
      <Label group="Include" match="#include\s*[&quot;']+([\w.\/]+)[&quot;']+\s*$" name="\1" image="15" navigation="false">
        <Containers open="preprocessor"/>
      </Label>
      <Label group="Function" match="(?:^|\s)((STATIC)\s+)?FUNCTION\s+(\w+)\s*(\([^.;)]*\))?(?:\s*//\s*([^\r\n)]+))?" name="\3" descr="\1Function \3 \4 \5" image="9" style="function" highlight="\3" scope="1">
        <SubImage if="\2" equal="STATIC" value="3"/>
      </Label>
      <Label group="Procedure" match="(?:^|\s)((STATIC|INIT|EXIT)\s+)?PROCEDURE\s+(\w+)\s*(\([^.;)]*\))?(?:\s*//\s*([^\r\n)]+))?" descr="\1PROCEDURE \3" name="\1Procedure \3 \4 \5" image="9" style="function" highlight="\3" scope="1">
        <SubImage if="\2" equal="STATIC" value="3"/>
      </Label>
      <Label group="Exits" match="\&lt;(loop|exit)\&gt;" name="\1" descr="\1" navigation="false" marker="19" context="exit_scope" visible="false"/>
      <Label group="Exits" match="\&lt;return\&gt;" name="\1" descr="\0" navigation="false" marker="20" context="exit_all" visible="false"/>
      <Label group="Function" match="(\w+)\(" name="\1" descr="\1" navigation="false" context="function_call" style="function" highlight="\1" visible="false"/>
      <Label group="Region" match="(?:^|\s)Region\s+(\w+)" name="\1" descr="Region \1" image="8" scope="1"/>
      <Label group="Region" match="(?:^|\s)Label\s+(\w+)\s+(\w+)" name="\2" descr="Label \1 \2" image="8" scope="1"/>
    </LABELS>
