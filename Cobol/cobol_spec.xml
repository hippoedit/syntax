<XMLConfigSettings>
  <FILEINFO author="HippoEDIT.com" type="LangSpec"/>
  <SYNTAX id="cobol" name="Cobol" inherit="def_source" inherit_url="defsource_spec.xml">
    <SPECIFICATION>
      <FilePattern mask="*.cbl;*.cob;*.cpy"/>
      <OpenClose>()[]""</OpenClose>
      <Words init="A-Za-z">0-9A-Za-z_-</Words>
      <LineEnd>.</LineEnd>      
      <Operators>@?%+/&amp;*=&lt;&gt;|!~^,;.:()[]</Operators>
    </SPECIFICATION>
    <SCOPES>
      <Scope open="evaluate" middle="when" close="end-evaluate"/>
      <Scope open="exec" close="end-exec"/>      
      <Scope open="if" middle="else" close="end-if">
        <Middle text="then"/>
      </Scope>
    </SCOPES>    
    <STYLES>
      <Style id="url" extend="true">
        <Containers>
          <Open id="normal" exclude="true"/>
          <Open id="string"/>
          <Open id="comment"/>
        </Containers>
      </Style>
      <Style id="comment" name="Comments" text="1" bold="0" italic="1" underline="0" clr="Comments" bkclr="#FFFFFFFF">
        <Blocks>
          <Block open="*" start_pos="6" close="\n"/>
        </Blocks>
      </Style>
      <Style id="string" name="Strings" text="1" bold="0" italic="0" underline="0" clr="Strings" bkclr="#FFFFFFFF">
        <Blocks>
          <Block open="&quot;" close="&quot;"/>
          <Block open="'" close="'"/>          
        </Blocks>
      </Style>
      <Style id="operator" name="Operator" bold="0" italic="0" underline="0" clr="Operator" bkclr="#FFFFFFFF"/>
