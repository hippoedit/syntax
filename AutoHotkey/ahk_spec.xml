<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="syntax.xslt"?>
<XMLConfigSettings>
  <FILEINFO author="HippoEDIT.com" type="LangSpec"/>
  <SYNTAX id="ahk" name="AutoHotkey" inherit="def_source" inherit_url="defsource_spec.xml" version="1.1">
    <SPECIFICATION>
      <FilePattern mask="*.ahk"/>
      <Operators>%=&lt;&gt;*{}()&amp;/.:!^+--,?|~</Operators>
      <OpenClose>()[]{}""</OpenClose>
      <EscapeChar>`</EscapeChar>
      <Bars navigation="true"/>
    </SPECIFICATION>
    <SCOPES>
      <Scope open="{" close="}"/>
    </SCOPES>
    <LABELS>
      <Label group="Exits" match="\&lt;(Break)\&gt;" name="\1" descr="\1" navigation="false" marker="19" context="exit_scope" visible="false"/>
      <Label group="Exits" match="\&lt;(ExitApp|Return)\&gt;" name="\1" descr="\1" navigation="false" marker="20" context="exit_all" visible="false"/>
      <Label group="Label" match="^\s*(?:~\*)?(\w+)\s*\:[^=]" name="\1" descr="Label \1" navigation="true" visible="true" style="label" image="13" highlight="\1"/>
      <Label group="Label" match="^\s*(?:~\*)?(\w+)\s*\:[^=]" name="\1" descr="Label \1" navigation="true" visible="true" style="label" image="13" highlight="\1"/>
      <Label group="HotString" match="^\s*\:.*?\:([^:\R]+?)\:\:" name="\1" descr="HotString \1" navigation="true" visible="true" style="hotstring" image="13" highlight="\1">
        <Containers open="hotstring"/>
      </Label>
      <Label group="HotKey" match="^\s*([\#\!\^\+&amp;&lt;&gt;\*\~\$\w][^:\R]+?)\:\:" name="\1" descr="HotKey \1" navigation="true" visible="true" style="hotkey" image="13" highlight="\1">
        <Containers open="hotkey"/>
      </Label>
      <Label group="Variable" match="%(\w+)%" name="\1" descr="\1" image="6" navigation="false" context="variable" style="variable" visible="false" highlight="\1">
        <Containers open="variable_value"/>
      </Label>
      <Label group="Function" match="^(\w+)\s*(\([^)]*\))\s*\{" name="\1" descr="\1 \2" scope="1" image="18" style="function" highlight="\1">
        <Skip if="\1" equal="if"/>
      </Label>
      <Label group="Function" match="(\w+)\(" name="\1" descr="\1" navigation="false" context="function_call" style="function" highlight="\1" visible="false"/>
    </LABELS>
    <STYLES>
      <Style id="string" name="Strings" text="1" bold="0" italic="0" underline="0" clr="Strings" bkclr="#FFFFFFFF">
        <Containers>
          <Open id="preprocessor"/>
          <Open id="normal"/>
        </Containers>
        <Blocks>
          <Block open="&quot;" close="&quot;"/>
        </Blocks>
      </Style>
      <Style id="merged_string" name="Merged Strings" text="1" bold="0" italic="0" underline="0" clr="Strings" bkclr="#FFFFFFFF">
        <Containers>
          <Open id="normal"/>
        </Containers>
        <Blocks>
          <Regexp open="^\(" close="^\)"/>
        </Blocks>
      </Style>
      <Style name="Merged Strings Keywords" bold="1" clr="#FFFFFFFF" bkclr="#FFFFFFFF" image="12">
        <Containers>
          <Open id="merged_string"/>
        </Containers>
        <Keywords>
          <Keyword text="Join" descr="Specifies how lines should be connected together"/>
          <Keyword text="LTrim" descr="Omits spaces and tabs at the beginning of each line"/>
          <Keyword text="RTrim0" descr="Turns off the omission of spaces and tabs from the end of each line"/>
          <Keyword text="Comments" descr="Allows semicolon comments inside the continuation section (but not /*..*/)"/>
          <Keyword text="Comment" descr="Allows semicolon comments inside the continuation section (but not /*..*/)"/>
          <Keyword text="Com" descr="Allows semicolon comments inside the continuation section (but not /*..*/)"/>
        </Keywords>
      </Style>
      <Style id="msg_box" name="MsgBox parameters" text="1" bold="0" italic="0" underline="0" clr="Strings" bkclr="#FFFFFFFF">
        <Containers>
          <Open id="normal"/>
        </Containers>
        <Blocks>
          <Regexp open="[\s,]+[^%;\d]" open_lead="(?:\s*,\s*\d+\s*)?(?:\s*,)?\s*xobgsm[^\w]?" close="$"/>
        </Blocks>
      </Style>
      <Style id="control_params" name="Control parameters" text="1" bold="0" italic="0" underline="0" clr="Strings" bkclr="#FFFFFFFF">
        <Containers>
          <Open id="normal"/>
        </Containers>
        <Blocks>
          <Regexp open="[\s,]+[^%;]" open_lead="(?:\s*,)?\s*(?:xobtupni|pitloot)(?:[^\w]|$)" close="$"/>
        </Blocks>
      </Style>
      <Style id="comment" name="Comments" text="1" bold="0" italic="1" underline="0" clr="Comments" bkclr="#FFFFFFFF">
        <Containers>
          <Open id="preprocessor"/>
          <Open id="normal"/>
        </Containers>
        <Blocks>
          <Block open=";" close="\n"/>
          <Block open="/*" close="*/"/>
        </Blocks>
      </Style>
      <Style id="preprocessor" name="Directives" bold="0" italic="0" underline="0" clr="Preprocessor" bkclr="#FFFFFFFF">
        <Block open="#" close="\n" first_pos="true"/>
      </Style>
      <Style name="Directive Keywords" bold="1" clr="#FFFFFFFF" bkclr="#FFFFFFFF" image="12">
        <Containers>
          <Open id="preprocessor"/>
        </Containers>
        <Keywords>
          <Keyword text="AllowSameLineComments" descr="Only for AutoIt v2 (.aut) scripts: Allows a comment to appear on the same line as a command"/>
          <Keyword text="ClipboardTimeout" descr="Changes how long the script keeps trying to access the clipboard when the first attempt fails"/>
          <Keyword text="CommentFlag" descr="Changes the script's comment symbol from semicolon to some other string"/>
          <Keyword text="ErrorStdOut" descr="Sends any syntax error that prevents a script from launching to stdout rather than displaying a dialog"/>
          <Keyword text="EscapeChar" descr="Changes the script's escape character (for example: backslash vs. accent)"/>
          <Keyword text="HotkeyInterval" descr="Along with MaxHotkeysPerInterval, specifies the rate of hotkey activations beyond which a warning dialog will be displayed"/>
          <Keyword text="HotkeyModifierTimeout" descr="Affects the behavior of hotkey modifiers: CTRL, ALT, WIN, and SHIFT"/>
          <Keyword text="Hotstring" descr="Changes hotstring options or ending characters"/>
          <Keyword text="IfWinActive" descr="Creates context-sensitive hotkeys and hotstrings. Such hotkeys perform a different action (or none at all) depending on the type of window that is active or exists"/>
          <Keyword text="IfWinExist" descr="Creates context-sensitive hotkeys and hotstrings. Such hotkeys perform a different action (or none at all) depending on the type of window that is active or exists"/>
          <Keyword text="Include" descr="Causes the script to behave as though the specified file's contents are present at this exact position"/>
          <Keyword text="InstallKeybdHook" descr="Forces the unconditional installation of the keyboard hook"/>
          <Keyword text="InstallMouseHook" descr="Forces the unconditional installation of the mouse hook"/>
          <Keyword text="KeyHistory" descr="Sets the maximum number of keyboard and mouse events displayed by the KeyHistory window. You can set it to 0 to disable key history"/>
          <Keyword text="MaxHotkeysPerInterval" descr="Along with #HotkeyInterval, specifies the rate of hotkey activations beyond which a warning dialog will be displayed"/>
          <Keyword text="MaxMem" descr="Sets the maximum capacity of each variable to the specified number of megabytes"/>
          <Keyword text="MaxThreads" descr="Sets the maximum number of simultaneous threads"/>
          <Keyword text="MaxThreadsBuffer" descr="Causes some or all hotkeys to buffer rather than ignore keypresses when their #MaxThreadsPerHotkey limit has been reached"/>
          <Keyword text="MaxThreadsPerHotkey" descr="Sets the maximum number of simultaneous threads per hotkey or hotstring"/>
          <Keyword text="NoEnv" descr="Avoids checking empty variables to see if they are environment variables (recommended for all new scripts)"/>
          <Keyword text="NoTrayIcon" descr="Disables the showing of a tray icon"/>
          <Keyword text="Persistent" descr="Keeps a script permanently running (that is, until the user closes it or ExitApp is encountered)"/>
          <Keyword text="SingleInstance" descr="Determines whether a script is allowed to run again when it is already running"/>
          <Keyword text="UseHook" descr="Forces the use of the hook to implement all or some keyboard hotkeys"/>
          <Keyword text="WinActivateForce"/>
        </Keywords>
      </Style>
      <Style id="operator" extend="true">
        <Containers>
          <Open id="preprocessor"/>
        </Containers>
      </Style>
