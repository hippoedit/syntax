<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="syntax.xslt"?>
<XMLConfigSettings>
  <FILEINFO author="HippoEDIT.com" type="LangSpec"/>
  <SYNTAX id="def" name="Default" abstract="true">
    <SPECIFICATION>
      <Words>0-9A-Za-z_</Words>
      <WrapBy>(),</WrapBy>
    </SPECIFICATION>
    <InsertDateTime>
      <Insert format="%d.%m.%Y %H:%M"/>
      <Insert format="%d.%m.%y %I:%M %p"/>
      <Insert format="%c"/>
      <Insert format="%#c"/>
      <Insert format="%#x"/>
    </InsertDateTime>
    <STYLES>
      <Style id="window" name="Window" dstyle="true" clr="Window Text" bkclr="Window Back" overview="3"/>
      <Style id="margin" name="Selection Margin" dstyle="true" dbkclr="true" clr="Margin" bkclr="Margin" overview="3"/>
      <Style id="selected" name="Selected Text" bold="2" italic="2" underline="2" clr="Selected Text" bkclr="Selected Back" overview="1"/>
      <Style id="selected_ina" name="Inactive Selected text" bold="2" italic="2" underline="2" clr="Selected Text Inactive" bkclr="Selected Back Inactive" overview="3"/>
      <Style id="line_number" name="Line Number" bold="2" italic="2" underline="2" clr="Line Number" bkclr="#FFFFFFFF" overview="3"/>
      <Style id="curr_line" name="Current Line" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Current Line" overview="1"/>
      <Style id="curr_line_frame" name="Current Line Frame" dstyle="true" dbkclr="true" clr="Current Line Frame" bkclr="Current Line Frame" overview="3"/>
      <Style id="outlining" name="Outlining" dstyle="true" clr="Outlining" bkclr="#FFFFFFFF" overview="3"/>
      <Style id="outlining_highlight" name="Outlining (Highlighted)" dstyle="true" clr="Outlining (Highlighted)" bkclr="#FFFFFFFF" overview="3"/>
      <Style id="read_only" name="Read-Only Window" dstyle="true" clr="Window Text" bkclr="Read Only Back" overview="3"/>
      <Style id="changed" name="Changed Indicator" dstyle="true" dbkclr="true" clr="Changed Indicator" bkclr="Changed Indicator" overview="0"/>
      <Style id="saved" name="Changed and Saved Indicator" dstyle="true" dbkclr="true" clr="Changed/Saved Indicator" bkclr="Changed/Saved Indicator" overview="0"/>
      <Style id="alternative" name="Alternative Color" dstyle="true" clr="Alternating" bkclr="Alternating" bold="2" italic="2" underline="2" overview="3"/>
      <Style id="max_line_length" name="Maximum Line Length" dstyle="true" dbkclr="true" clr="Error" bkclr="Error" overview="3"/>
      <Style id="bookmark" name="Bookmark" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Bookmark" overview="1"/>
      <Style id="search_result" name="Search Result" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Search Result" overview="1"/>
      <Style id="incremental_search_result1" name="Incremental Search Result 1" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Incremental Search Result 1" overview="1"/>
      <Style id="incremental_search_result2" name="Incremental Search Result 2" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Incremental Search Result 2" overview="1"/>
      <Style id="smart_highlight" name="Smart Highlight" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Incremental Search Result 2" overview="1"/>
      <Style id="highlight1" name="Highlight 1" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Highlight 1" overview="1"/>
      <Style id="highlight2" name="Highlight 2" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Highlight 2" overview="1"/>
      <Style id="highlight3" name="Highlight 3" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Highlight 3" overview="1"/>
      <Style id="highlight4" name="Highlight 4" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Highlight 4" overview="1"/>
      <Style id="highlight5" name="Highlight 5" bold="2" italic="2" underline="2" clr="#FFFFFFFF" bkclr="Highlight 5" overview="1"/>
      <Style id="white_space" name="Visible White Space" clr="Format tag" bkclr="#FFFFFFFF" bold="2" italic="2" underline="2" overview="3"/>
      <Style id="line_break" name="Line Break" clr="Format tag" bkclr="#FFFFFFFF" bold="2" italic="2" underline="2" overview="3"/>
      <Style id="trailing_white_space" name="Trailing White Space" clr="Error" bkclr="#FFFFFFFF" bold="2" italic="2" underline="2" overview="3"/>
      <Style id="auto_correction" name="Auto Correction Text (Completion List)" clr="$1000" bkclr="#FFFFFFFF" dstyle="true" image="6" overview="3"/>
      <Style id="template" name="Template (Completion List)" clr="#FFFFFFFF" bkclr="#FFFFFFFF" dstyle="true" image="15" overview="3"/>
      <Style id="normal" name="Normal" dbkclr="false" bold="0" italic="0" underline="0" clr="Window Text" bkclr="#FFFFFFFF" image="6" overview="3"/>
      <Style id="inactive_code" name="Inactive Code" bold="2" italic="2" underline="2" clr="Inactive Code" bkclr="#FFFFFFFF" overview="3"/>
      <Style id="syntax_error" name="Syntax Error" clr="#FFFFFFFF" bkclr="#FFFFFFFF" overview="2" underline_style="3" underline_clr="Error" italic="2" underline="2" bold="2" abstract="true"/>
      <Style id="syntax_warning" name="Syntax Warning" clr="#FFFFFFFF" bkclr="#FFFFFFFF" overview="2" underline_style="3" underline_clr="#0000FF" italic="2" underline="2" bold="2" abstract="true"/>
      <Style id="syntax_info" name="Syntax Info" clr="#FFFFFFFF" bkclr="#FFFFFFFF" overview="2" underline_style="3" underline_clr="#00FF00" italic="2" underline="2" bold="2" abstract="true"/>
      <Style id="url" name="WWW addresses and E-Mails" text="2" underline="1" clr="#FFFFFFFF" bkclr="#FFFFFFFF" hotspot="1" bold="2" italic="2">
        <Blocks>
          <Block open="http://">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
          <Block open="ftp://">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
          <Block open="sftp://">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
          <Block open="https://">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
          <Block open="file://">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
          <Block open="irc://">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
          <Block open="mailto:">
            <Close noneof="0-9A-Za-z--._~%:\/?#[]@!$&amp;*+,;="/>
          </Block>
        </Blocks>
      </Style>
    </STYLES>
    <FONTS>
      <Font only_monospaced="false" size="10" quality="0" charset="1"/>
    </FONTS>
  </SYNTAX>
</XMLConfigSettings>
