  <SYNTAX id="groovy" name="Groovy" inherit="def_source" inherit_url="defsource_spec.xml" version="1.1">
    <SPECIFICATION>
      <FilePattern mask="*.groovy;*.gvy;*.gy"/>
      <CaseSensitive>true</CaseSensitive>
      <EscapeChar>\</EscapeChar>
      <Delimiters>~!@%^&amp;*()-+=|\/{}[]:;&quot;'&lt;&gt;,?</Delimiters>
      <Operators>()[]{}+-*/%=&lt;&gt;&amp;|!.~</Operators>
      <Bars navigation="true"/>
    </SPECIFICATION>
    <LABELS>
      <Label group="Definition" match="\&lt;(def|class)\s+(\w+)\s*(\([^)]*\))\s*\{" name="\2" descr="\1 \2\3" image="5" scope="1" style="function" highlight="\2">
        <Image if="\1" equal="def" value="6"/>
      </Label>
      <Label group="Function" match="(\w+)\(" name="\1" descr="\1" navigation="false" context="function_call" style="function" highlight="\1" visible="false"/>
      <Label group="Variable" match="\&lt;def\s+([^=;\s]+)\s*=" name="\1" descr="\1" image="6" navigation="false" context="variable" style="variable" visible="false">
        <Highlight in="\1" all="\w+" style="variable" global="false"/>
      </Label>
    </LABELS>
    <SCOPES>
      <Scope open="{" close="}"/>
    </SCOPES>
